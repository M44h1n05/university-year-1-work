# Store decimal values 42 and 9 in registers s1 and s2 respectively
# s1 = 0 + 42
addi s1, zero, 42
#s2 = 0 + 9
addi s2, zero, 9

# Add them together and store the result in s3
# s3 = s1+ s2
add s3, s1, s2

# Add 10 to the result and store back in s3
#s3 = s3 + 10
addi s3, s3, 10

# store value 5 and 12 in registers t3 and t4 respectively
addi s10, zero , 0 
addi t3, zero, 5 
addi t4, zero, 12
addi t5, zero, 3 
addi t6, zero ,2

add s10, t3, t4
add s10, s10,t5
