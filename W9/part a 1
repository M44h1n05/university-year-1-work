.data

enterNumMsg: .string "Please enter a whole number "
addr: .string " + "
equals: .string " = "

.text

addi a7, zero, 4 #stores value for print string in a7
la a0 enterNumMsg #load address of string using its label
ecall

addi a7, zero, 5
ecall

add s2, zero, a0

addi s3, s2, 1

addi a7,  zero, 1
add a0 , zero, s2 
ecall

addi a7, zero, 4
la a0 , addr
ecall

addi a7, zero, 1
addi a0, zero, 1
ecall

addi, a7, zero , 4
la a0, equals 
ecall

addi a7,zero, 1 
add a0, zero, s3
ecall